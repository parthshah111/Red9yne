<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="544.0" prefWidth="839.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.SuppliersController">
    <children>
        <TableView fx:id="tableView" layoutX="265.0" layoutY="45.0" onMouseClicked="#userClickedOnTable" prefHeight="455.0" prefWidth="460.0">
            <columns>
                <TableColumn fx:id="supplierName" prefWidth="262.0" text="Supplier Name" />
                <TableColumn fx:id="status" prefWidth="197.0" text="Status" />
            </columns>
        </TableView>
        <Label layoutX="14.0" layoutY="45.0" text="Supplier Status">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Label>
        <Button fx:id="backtoMM" layoutX="21.0" layoutY="463.0" mnemonicParsing="false" onAction="#backtoMM" prefHeight="27.0" prefWidth="137.0" text="Back to Main Menu" />
        <Label layoutX="14.0" layoutY="88.0" text="Supplier Name" />
        <TextField fx:id="sName" layoutX="102.0" layoutY="84.0" />
        <Button fx:id="addSupplier" layoutX="21.0" layoutY="197.0" mnemonicParsing="false" onAction="#addSupplier" text="Add Supplier" />
        <ChoiceBox fx:id="sStatus" layoutX="101.0" layoutY="121.0" prefWidth="150.0" />
        <Label layoutX="14.0" layoutY="125.0" text="Status" />
        <Button fx:id="deleteSupplier" layoutX="21.0" layoutY="233.0" mnemonicParsing="false" onAction="#deleteSupplier" text="Delete Supplier" />
        <Button fx:id="updateSupplier" layoutX="21.0" layoutY="270.0" mnemonicParsing="false" onAction="#updateSupplier" text="Update Supplier" />
        <Button fx:id="clear" layoutX="199.0" layoutY="197.0" mnemonicParsing="false" onAction="#clear" text="Clear" />
    </children>
</AnchorPane>